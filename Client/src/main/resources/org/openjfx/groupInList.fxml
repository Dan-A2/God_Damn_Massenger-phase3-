<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="50.0" prefWidth="640.0" style="-fx-background-color: #642194;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.openjfx.view.lists.chat.GroupForListView">
   <children>
      <Label fx:id="groupNameLabel" prefHeight="50.0" prefWidth="240.0" text="group name" textFill="WHITE">
         <font>
            <Font name="Cambria" size="17.0" />
         </font>
         <padding>
            <Insets left="10.0" />
         </padding>
      </Label>
      <Label fx:id="unseenLabel" alignment="CENTER" layoutX="485.0" prefHeight="50.0" prefWidth="41.0" text="0" textFill="WHITE">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Button fx:id="viewBtn" layoutX="588.0" mnemonicParsing="false" onAction="#view" prefHeight="50.0" prefWidth="52.0" style="-fx-background-color: #327f8e;" text="view">
         <font>
            <Font name="Cambria" size="14.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="leaveBtn" layoutX="527.0" mnemonicParsing="false" onAction="#leave" prefHeight="50.0" prefWidth="61.0" style="-fx-background-color: red;" text="leave">
         <font>
            <Font name="Cambria" size="16.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
