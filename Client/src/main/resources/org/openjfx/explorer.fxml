<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane fx:id="explorerPane" prefHeight="600.0" prefWidth="650.0" style="-fx-background-color: #006400;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.openjfx.view.explorer.ExplorerView">
   <children>
      <Label alignment="CENTER" prefHeight="49.0" prefWidth="650.0" text="Explorer" textFill="GREENYELLOW">
         <font>
            <Font name="Algerian" size="36.0" />
         </font>
      </Label>
      <Button fx:id="randTweetBtn" layoutX="101.0" layoutY="285.0" mnemonicParsing="false" onAction="#showRand" style="-fx-background-color: #9ACD32;" text="rendom tweetPage">
         <font>
            <Font name="Cambria Bold" size="16.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="searchBtn" layoutX="482.0" layoutY="285.0" mnemonicParsing="false" onAction="#showSearch" style="-fx-background-color: #9ACD32;" text="search">
         <font>
            <Font name="Cambria Bold" size="16.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
